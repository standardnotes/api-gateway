    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "inversify": "^5.0.5",
    "inversify-express-utils": "^6.3.2",
    "jsonwebtoken": "^8.5.1",
    "newrelic": "^7.3.1",
    "prettyjson": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@standardnotes/config": "2.0.1",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/newrelic": "^7.0.1",
    "@types/prettyjson": "^0.0.29",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "pre-push": "^0.1.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "pre-push": [
